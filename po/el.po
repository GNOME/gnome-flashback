msgstr "Ε_ντάξει"

#: gnome-flashback/libautomount-manager/gsd-autorun.c:790
msgid "_Eject"
msgstr "Ε_ξαγωγή"

#: gnome-flashback/libautomount-manager/gsd-autorun.c:795
msgid "_Unmount"
